{
    "name": "nuxt-starter",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "nuxt",
        "build": "nuxt build",
        "start": "nuxt start",
        "generate": "nuxt generate",
        "lint:js": "eslint . --fix --ext .ts,.vue,.tsx",
        "lint:style": "stylelint --fix \"**/*.{vue,css,scss}\"",
        "lint": "npm run lint:js && npm run lint:style",
        "prepare": "husky install",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
        "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open"
    },
    "lint-staged": {
        "*.{ts,tsx,vue}": "eslint",
        "*.{css,vue,scss}": "stylelint"
    },
    "dependencies": {
        "@mdi/js": "^5.9.55",
        "@nuxtjs/composition-api": "^0.23.4",
        "@nuxtjs/pwa": "^3.3.5",
        "@nuxtjs/sitemap": "^2.4.0",
        "core-js": "^3.11.1",
        "nuxt": "^2.15.3"
    },
    "devDependencies": {
        "@nuxt/image": "^0.4.8",
        "@nuxt/types": "^2.15.3",
        "@nuxt/typescript-build": "^2.1.0",
        "@nuxtjs/color-mode": "^2.0.5",
        "@nuxtjs/eslint-config-typescript": "^6.0.0",
        "@nuxtjs/eslint-module": "^3.0.2",
        "@nuxtjs/robots": "^2.5.0",
        "@nuxtjs/stylelint-module": "^4.0.0",
        "@nuxtjs/tailwindcss": "^4.0.1",
        "@types/tailwindcss": "^2.0.2",
        "babel-eslint": "^10.1.0",
        "cypress": "^7.2.0",
        "eslint": "^7.27.0",
        "eslint-plugin-cypress": "^2.11.2",
        "eslint-plugin-nuxt": "^2.0.0",
        "eslint-plugin-vue": "^7.7.0",
        "fibers": "^5.0.0",
        "husky": "^6.0.0",
        "lint-staged": "^10.5.4",
        "postcss": "^8.2.13",
        "sass": "^1.32.12",
        "sass-loader": "^10.1.1",
        "start-server-and-test": "^1.12.1",
        "stylelint": "^13.12.0",
        "stylelint-config-standard": "^22.0.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    }
}
